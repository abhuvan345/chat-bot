{
  "manifest_version": 3,
  "name": "MCQ Helper",
  "version": "1.0",
  "description": "Get answers to MCQ questions with AI",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ]
}